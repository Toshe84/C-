using System;

namespace Solution_1_2_Fact
{
    class Program
    {
        static void Main(string[] args)
        {
            Factorial(5);
        }
        public static int Factorial(int n)
        {
            if (n == 0)
            {
                return 1;
        }
            else
            {
                return n * Factorial(n - 1);
            }

        }
    }
}
